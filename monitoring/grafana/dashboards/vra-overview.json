{
  "title": "VRA Overview",
  "editable": true,
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": { "from": "now-6h", "to": "now" },
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "VRA Query Duration (op)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(vra_query_duration_seconds_bucket[5m])) by (le,op))", "legendFormat": "p95 {{op}}" },
        { "expr": "sum(rate(vra_query_duration_seconds_sum[5m])) / sum(rate(vra_query_duration_seconds_count[5m]))", "legendFormat": "avg all" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Matching — auto/review/unmatched",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        { "expr": "rate(vra_match_auto_total[5m])", "legendFormat": "auto" },
        { "expr": "rate(vra_match_review_total[5m])", "legendFormat": "review" },
        { "expr": "rate(vra_match_unmatched_total[5m])", "legendFormat": "unmatched" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Reconcile — rows by kind",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(vra_reconcile_rows_total[5m])) by (kind)", "legendFormat": "{{kind}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Proofs — coverage % (daily)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        { "expr": "vra_proofs_coverage_pct", "legendFormat": "{{day}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ingest — rows parsed / loads / failures",
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 },
      "targets": [
        { "expr": "sum(rate(vra_statements_rows_parsed_total[5m])) by (network)", "legendFormat": "parsed {{network}}" },
        { "expr": "sum(rate(vra_statements_loads_total[5m])) by (phase)", "legendFormat": "loads {{phase}}" },
        { "expr": "sum(rate(vra_statements_load_failures_total[5m])) by (reason)", "legendFormat": "fail {{reason}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Expected Builder — duration (p95)",
      "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(vra_expected_build_duration_seconds_bucket[5m])) by (le,outcome))", "legendFormat": "p50 {{outcome}}" },
        { "expr": "histogram_quantile(0.95, sum(rate(vra_expected_build_duration_seconds_bucket[5m])) by (le,outcome))", "legendFormat": "p95 {{outcome}}" },
        { "expr": "histogram_quantile(0.99, sum(rate(vra_expected_build_duration_seconds_bucket[5m])) by (le,outcome))", "legendFormat": "p99 {{outcome}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Reconcile — duration (p95)",
      "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(vra_reconcile_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p50" },
        { "expr": "histogram_quantile(0.95, sum(rate(vra_reconcile_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95" },
        { "expr": "histogram_quantile(0.99, sum(rate(vra_reconcile_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p99" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Matching — duration (p95)",
      "gridPos": { "x": 0, "y": 32, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(vra_match_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p50" },
        { "expr": "histogram_quantile(0.95, sum(rate(vra_match_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95" },
        { "expr": "histogram_quantile(0.99, sum(rate(vra_match_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p99" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Proofs — issuance duration (p95)",
      "gridPos": { "x": 12, "y": 32, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(vra_proofs_issuance_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p50" },
        { "expr": "histogram_quantile(0.95, sum(rate(vra_proofs_issuance_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95" },
        { "expr": "histogram_quantile(0.99, sum(rate(vra_proofs_issuance_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p99" }
      ]
    }
  ]
}
