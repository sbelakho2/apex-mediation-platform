{
  "title": "VRA Ingest by Network",
  "editable": true,
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": { "from": "now-6h", "to": "now" },
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Rows Parsed per Network (rps)",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 8 },
      "targets": [
        { "expr": "sum(rate(vra_statements_rows_parsed_total[5m])) by (network)", "legendFormat": "{{network}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Loads Success (raw/norm)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(vra_statements_loads_total[5m])) by (phase)", "legendFormat": "{{phase}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Load Failures by Reason",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(vra_statements_load_failures_total[5m])) by (reason)", "legendFormat": "{{reason}}" }
      ]
    }
  ]
}
