{
  "name": "@rivalapex/web-sdk",
  "version": "0.1.0",
  "description": "Rival Apex Mediation Web SDK (consent, auction client, errors, events)",
  "private": false,
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist coverage artifacts || true",
    "build": "npm run clean && tsc -p tsconfig.build.json && rollup -c",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest --config jest.config.cjs",
    "docs": "typedoc --out typedoc src/index.ts",
    "gen:mock-trace": "node -e \"const fs=require('fs');fs.mkdirSync('artifacts',{recursive:true});fs.writeFileSync('artifacts/mock-success-trace.json', JSON.stringify({ok:true,timestamp:Date.now()},null,2))\""
  },
  "keywords": [
    "ad-mediation",
    "web-sdk",
    "ads",
    "auction",
    "consent"
  ],
  "author": "Bel Consulting OÃœ",
  "license": "PROPRIETARY",
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "msw": "^1.3.4",
    "abort-controller": "^3.0.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "rollup": "^4.24.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "tslib": "^2.6.3",
    "typescript": "^5.9.3",
    "whatwg-fetch": "^3.6.20",
    "typedoc": "^0.25.13"
  }
}
