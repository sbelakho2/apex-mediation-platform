{
  "version": "2",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next-public-api-url",
    "NEXT_PUBLIC_CONSOLE_URL": "@next-public-console-url",
    "NEXT_PUBLIC_SITE_URL": "@next-public-site-url"
  },
  "regions": ["arn1"],
  "framework": "nextjs"
}
